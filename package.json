{
  "name": "template-loop-card",
  "version": "0.1.0",
  "description": "A Home Assistant custom card that loops over template-generated lists",
  "main": "dist/template-loop-card.js",
  "scripts": {
    "build": "tsc && vite build",
    "build:no-check": "vite build",
    "watch": "vite build --mode development",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "home-assistant",
    "hacs",
    "custom-cards",
    "template",
    "loop"
  ],
  "author": "Sebastiaan Pasma",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "prettier": "^3.5.3",
    "typescript": "^5.3.2",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "custom-card-helpers": "^1.9.0",
    "lit": "3.3.0"
  }
}
